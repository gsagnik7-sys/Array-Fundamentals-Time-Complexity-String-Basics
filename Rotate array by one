class Solution:
    def rotate(self, arr):
        if len(arr) == 0:
            return
        last = arr[-1]
        for i in range(len(arr) - 1, 0, -1):
            arr[i] = arr[i - 1]
        arr[0] = last
        return arr

c-->
void rotate(int arr[], int n) {
    if (n == 0) return;
    int last = arr[n - 1];
    for (int i = n - 1; i > 0; i--) {
        arr[i] = arr[i - 1];
    }
    arr[0] = last;
}
