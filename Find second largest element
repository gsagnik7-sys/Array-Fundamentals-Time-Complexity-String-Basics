class Solution:
    def getSecondLargest(self, arr):
        first = second = -1
        for num in arr:
            if num > first:
                second = first
                first = num
            elif num > second and num != first:
                second = num
        return second

c-->
int getSecondLargest(int arr[], int n) {
    int first = -1, second = -1;
    for (int i = 0; i < n; i++) {
        if (arr[i] > first) {
            second = first;
            first = arr[i];
        } else if (arr[i] > second && arr[i] != first) {
            second = arr[i];
        }
    }
    return second;
}
